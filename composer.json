{
    "name": "juandmegon/lumenapi",
    "description": "Curso de API RESTful con Lumen y Homestead.",
    "keywords": ["framework", "laravel", "lumen", "homestead", "restful", "api"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "laravel/homestead": "^2.1",
        "optimus/oauth2-server-lumen": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
